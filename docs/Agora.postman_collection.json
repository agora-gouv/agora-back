		{
			"name": "Login",
			"item": [
				{
					"name": "/signup",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"",
									"    var jsonData = pm.response.json();",
									"    pm.environment.set(\"jwtToken\", \"Bearer \"+jsonData.jwtToken);",
									"    pm.environment.set(\"loginToken\", jsonData.loginToken);",
									"    pm.environment.set(\"urlEncodedLoginToken\", encodeURIComponent(jsonData.loginToken));",
									"    pm.environment.set(\"userId\", jsonData.userId);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "fcmToken",
								"value": "{{fcmToken}}",
								"type": "text"
							},
							{
								"key": "versionName",
								"value": "",
								"type": "text",
								"disabled": true
							},
							{
								"key": "versionCode",
								"value": "1",
								"type": "text"
							},
							{
								"key": "platform",
								"value": "web",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{scheme}}://{{baseUrl}}/signup",
							"protocol": "{{scheme}}",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
